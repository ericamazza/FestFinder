<h1>Add a new Festival</h1>


<%= form_tag "/MyFestivals", :method => "post" do %>
  
  <%= label_tag "name" %> <%= text_field_tag "name" %>
  <%= label_tag "date" %> <%= text_field_tag "date" %>
  <%= label_tag "city" %> <%= text_field_tag "city" %>
  <%= label_tag "state" %> <%= text_field_tag "state" %>
  
  <%= submit_tag "Save" %>

<% end %>