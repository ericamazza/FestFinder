<h1>Edit <%= @fest.name %> </h1>


<%= form_tag "/MyFestivals/#{@fest.id}", :method => "put" do %>
  
  <%= label_tag "name" %> <%= text_field_tag "name", @fest.name %>
  <%= label_tag "date" %> <%= text_field_tag "date", @fest.date %>
  <%= label_tag "city" %> <%= text_field_tag "city", @fest.city %>
  <%= label_tag "state" %> <%= text_field_tag "state", @fest.state %>
  
  <%= submit_tag "Save" %>

<% end %>